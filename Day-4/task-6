import random
User_Num = int(input("Enter your number: Type 1 for rock, 2 for scissor, 3 for paper.\n "))
computer_Num = random.randint(1,3)
print(f"computer number: {computer_Num}")
if User_Num == 1 and computer_Num == 2:
    print("You wins!!, Computer lost.")
elif User_Num == 1 and computer_Num == 3:
    print("Computer wins!!, You lost")
elif User_Num == 2 and computer_Num == 1:
    print("COmputer wins!!, You lost")
elif User_Num == 2 and computer_Num == 3:
    print("You wins!!, Computer lost")
elif User_Num == 3 and computer_Num == 1:
    print("Computer wins!!, You lost ")   
elif User_Num == 3 and computer_Num == 2:  
    print("Computer wins!!, You Lost") 
elif User_Num == computer_Num:
    print("Tie between You and computer. ")
else:
    print("Out of the number")